[sub_resource type="GDScript" id="GDScript_n2iuh"]
resource_name = "StartGame"
script/source = "extends TextureButton

func _on_pressed() -> void:
	if not TitleScreenManager.canInteract or GamePopup.current:
		return
	if not Save.loadValue(\"completedTutorial\", false):
		GamePopup.openPopup(\"ConfirmationPrompt\", {
			\"title\": \"Play the tutorial first?\",
			\"description\": \"It looks like you haven't played the tutorial. Are you sure you want to continue?\",
			\"cancelText\": \"Go Back\",
			\"confirmText\": \"Play Game\",
			\"signalID\": \"NoTutorial\"
		})
		return
	GamePopup.openPopup(\"StartGame\")

func _on_mouse_entered() -> void:
	get_parent().self_modulate = Color(0.7, 0.7, 0.7)

func _on_mouse_exited() -> void:
	get_parent().self_modulate = Color.WHITE

func _ready() -> void:
	ConfirmationPrompt.onAccept(\"NoTutorial\", func():
		await TimeManager.wait(0.25)
		GamePopup.openPopup(\"StartGame\")
	)
"

[sub_resource type="GDScript" id="GDScript_8j3o8"]
resource_name = "StartGame"
script/source = "extends TextureButton

@export var popupName: String
@export var popupData: Dictionary
@export var signalID: String
@export var targetScene: String
@export var temporaryFlag: String

func _on_pressed() -> void:
	if not TitleScreenManager.canInteract or GamePopup.current:
		return
	popupData[\"signalID\"] = signalID
	GamePopup.openPopup(popupName, popupData)

func _on_mouse_entered() -> void:
	get_parent().self_modulate = Color(0.7, 0.7, 0.7)

func _on_mouse_exited() -> void:
	get_parent().self_modulate = Color.WHITE

func _ready() -> void:
	if not signalID.is_empty():
		ConfirmationPrompt.onAccept(signalID, func():
			Save.setTemporaryValue(temporaryFlag, true)
			var fadeOut: ColorRect = $\"../../../ScreenUI/FadeOut\"
			fadeOut.show()
			fadeOut.color = Color(0, 0, 0, 0)
			TitleScreenManager.canInteract = false
			var tween = NodeRelations.createTween()
			tween.tween_property(fadeOut, \"color\", Color.BLACK, 0.75)
			await TimeManager.wait(1.0)
			GamePopup.closeCurrent()
			NodeRelations.loadScene(targetScene)
		)
"

