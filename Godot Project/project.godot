; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false
warnings/check_angle_interpolation_type_conflicting=false

[application]

config/name="Barren"
run/main_scene="res://Scenes/Intro.tscn"
run/print_header=false
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://UI/BarrenLogo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://UI/BarrenIcon.png"

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/stretch/mode="viewport"

[editor]

export/convert_text_resources_to_binary=false

[file_customization]

folder_colors={
"res://Environment/": "yellow",
"res://HelperClasses/": "pink",
"res://Items/": "teal",
"res://ModelContents/": "red",
"res://ModelContents/AmmoDispenser/": "orange",
"res://ModelContents/AmmoDrop/": "orange",
"res://ModelContents/Cash/": "orange",
"res://ModelContents/Dummy/": "orange",
"res://ModelContents/Player/": "yellow",
"res://ModelContents/VanShop/": "orange",
"res://Models/": "gray",
"res://Particles/": "pink",
"res://Scenes/": "green",
"res://UI/": "purple",
"res://Upgrades/": "yellow",
"res://Weapons/": "gray",
"res://WeaponsContents/": "red",
"res://WeaponsContents/AK47/": "pink",
"res://WeaponsContents/MachineGun/": "pink",
"res://WeaponsContents/Shotgun/": "pink"
}

[global_group]

NavigationGroup="Nodes in this group will have all collisions used in pathfinding."

[gui]

theme/default_font_multichannel_signed_distance_field=true

[physics]

2d/default_gravity=0.0
2d/default_gravity_vector=Vector2(0, 0)

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
